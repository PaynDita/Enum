hashid -m <hashvalue>

hashcat -m <hashmode> hash.txt

hashcat -m <hashmode> --show hash.txt

hashcat hash.txt | grep hashmode (kerveros, MD5, Raw-MD5)

---------------------------------------------------------------------------------------------

john --format=raw-md5  --wordlist=/usr/share/wordlists/rockyou.txt hash.txt

john --format=raw-md5  --wordlist=/usr/share/wordlists/rockyou.txt hash.txt --pot=newrun.pot

john --show hash.txt

